{
    "ace": {
        "@behavioursEnabled": true,
        "@fontSize": 13,
        "@keyboardmode": "default",
        "@showFoldWidgets": false,
        "@showGutter": true,
        "@showInvisibles": false,
        "@showPrintMargin": false,
        "@theme": "ace/theme/cloud9_day",
        "statusbar": {
            "@show": "true"
        }
    },
    "clipboard": {
        "@dontshow": true
    },
    "collab": {
        "@timeslider-visible": false
    },
    "cs50": {
        "presentation": {
            "@editorFontSize": 24,
            "@presenting": false,
            "@terminalFontSize": 24
        }
    },
    "debug": {
        "@pause": 2
    },
    "general": {
        "@skin": "flat-light"
    },
    "imgeditor": {
        "@smooth": false,
        "@zoom": 100
    },
    "language": {
        "@continuousCompletion": false,
        "@enterCompletion": false
    },
    "openfiles": {
        "@show": false
    },
    "output": {},
    "projecttree": {
        "@showhidden": false
    },
    "tabs": {
        "@asterisk": false,
        "@show": true,
        "editorTypes": {
            "@class": "none",
            "@exe": "none",
            "@gz": "none",
            "@o": "none",
            "@pyc": "none",
            "@raw": "none",
            "@tar": "none",
            "@zip": "none"
        }
    },
    "terminal": {
        "@fontsize": 13,
        "@scrollback": 2000,
        "noclosequestion": {
            "json()": "true"
        }
    }
}